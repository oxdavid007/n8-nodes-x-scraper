{
  "name": "n8n-nodes-x-scraper",
  "version": "2.0.1",
  "description": "Scrape X Data with Lab3",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Scraper AI by Lab3",
    "email": "support@lab3.ai"
  },
  "repository": {
    "type": "git",
